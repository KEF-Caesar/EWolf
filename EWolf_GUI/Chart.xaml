<Window x:Class="EWolf_GUI.Chart"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EWolf_GUI"
        mc:Ignorable="d"
        Title="Chart" Height="450" Width="600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <WindowsFormsHost Grid.Row="0" Margin="10" Name="chartarea"/>
        <Button x:Name="closechart" Grid.Row="1" HorizontalAlignment="Right" Margin="0 0 10 10" Content="Close" Height="20" Width="60" Click="closechart_Click" FontSize="14"/>
    </Grid>
</Window>
